<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>Calc</servlet-name>
        <servlet-class>homework3.CalcServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Calc</servlet-name>
        <url-pattern>/calc</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Image</servlet-name>
        <servlet-class>homework3.ImagServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Image</servlet-name>
        <url-pattern>/Images</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Money</servlet-name>
        <servlet-class>homework4.MoneyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Money</servlet-name>
        <url-pattern>/money</url-pattern>
    </servlet-mapping>



</web-app>